{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c && cp public/index.html public/200.html",
    "dev": "rollup -c -w",
    "predeploy": "npm run build && cp public/index.html public/200.html",
    "deploy": "firebase deploy --token $FIREBASETOKEN --only hosting",
    "login:ci": "firebase login:ci",
    "count": "npx cloc --force-lang=\"HTML\",svelte src",
    "start": "sirv public --single -p 4200"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.4.2",
    "dotenv": "^8.6.0",
    "rollup": "^2.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.62.1",
    "svelte": "^3.59.1"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "export-to-csv": "^0.2.1",
    "firebase": "^9.22.1",
    "firebase-tools": "^12.4.0",
    "npm": "^9.6.7",
    "query-string": "^6.14.1",
    "ramda": "^0.27.2",
    "sirv-cli": "^0.4.6",
    "slug": "^2.1.1",
    "svelte-lighthouse-viewer": "^0.1.216",
    "svelte-preprocess": "^3.9.12",
    "svelte-router-spa": "^5.8.4"
  }
}
